{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::#DATA_BUCKET#",
        "arn:aws:s3:::#DATA_BUCKET#/*"
      ],
      "Condition": {
        "StringLike": {
          "aws:PrincipalArn": [
            "arn:aws:iam::#AWS_ACCOUNT_ID#:role/BenchmarkingStack-*",
            "arn:aws:iam::#AWS_ACCOUNT_ID#:role/redshift-*"
          ]
        }
      }
    }
  ]
}